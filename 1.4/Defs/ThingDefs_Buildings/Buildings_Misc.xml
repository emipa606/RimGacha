<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BenchBase">
    <defName>GachaMachine</defName>
    <label>gacha machine</label>
    <description>A large gacha machine to get sculptures from. Gamble for art, collect them all and create sets to display for even more beauty!</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>Things/Building/Misc/GachaMachine</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>false</castEdgeShadows>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.45</fillPercent>
    <passability>PassThroughOnly</passability>
    <pathCost>60</pathCost>
    <useHitPoints>True</useHitPoints>
    <size>(2,2)</size>
    <statBases>
      <WorkToBuild>2000</WorkToBuild>
      <MaxHitPoints>120</MaxHitPoints>
      <Flammability>0.8</Flammability>
    </statBases>
    <costList>
      <Steel>80</Steel>
      <ComponentIndustrial>2</ComponentIndustrial>
      <WoodLog>20</WoodLog>
    </costList>
    <constructionSkillPrerequisite>3</constructionSkillPrerequisite>
    <minTechLevelToBuild>Industrial</minTechLevelToBuild>
    <designationCategory>Misc</designationCategory>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(1,0,-1)</interactionCellOffset>
    <interactionCellIcon />
    <surfaceType>Item</surfaceType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
  </ThingDef>
</Defs>
